<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.nightowlscheduler">

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/Theme.NightOwlScheduler">

        <receiver
                android:name=".UI.MyReceiver"
                android:enabled="true"
                android:exported="true"></receiver>

        <activity
                android:name=".UI.Activity.AssessmentsDetailActivity"
                android:exported="false"
                android:label="Part List"
                android:parentActivityName=".UI.Activity.AssessmentsListActivity" />

        <activity
                android:name=".UI.Activity.AssessmentsAddActivity"
                android:exported="false"
                android:label="Part List"
                android:parentActivityName=".UI.Activity.AssessmentsListActivity" />

        <activity
                android:name=".UI.Activity.AssessmentsListActivity"
                android:exported="false"
                android:label="Part List"
                android:parentActivityName=".UI.Activity.CoursesDetailActivity" />

        <activity
                android:name=".UI.Activity.MentorsListActivity"
                android:exported="false"
                android:label="Part List"
                android:parentActivityName=".UI.Activity.CoursesDetailActivity" />

        <activity
                android:name=".UI.Activity.NotesDetailActivity"
                android:exported="false"
                android:label="Part List"
                android:parentActivityName=".UI.Activity.NotesListActivity" />

        <activity
                android:name=".UI.Activity.NotesAddActivity"
                android:exported="false"
                android:label="Part List"
                android:parentActivityName=".UI.Activity.NotesListActivity" />

        <activity
                android:name=".UI.Activity.NotesListActivity"
                android:exported="false"
                android:label="Part List"
                android:parentActivityName=".UI.Activity.CoursesDetailActivity" />

        <activity
                android:name=".UI.Activity.CoursesDetailActivity"
                android:exported="false"
                android:label="Part List"
                android:parentActivityName=".UI.Activity.CoursesListActivity" />

        <activity
                android:name=".UI.Activity.CoursesAddActivity"
                android:exported="false"
                android:label="Part List"
                android:parentActivityName=".UI.Activity.CoursesListActivity" />

        <activity
                android:name=".UI.Activity.CoursesListActivity"
                android:exported="false"
                android:label="Part List"
                android:parentActivityName=".UI.Activity.TermsListDetailActivity" />

        <activity
                android:name=".UI.Activity.TermsListDetailActivity"
                android:exported="false"
                android:label="Part List"
                android:parentActivityName=".UI.Activity.TermsListActivity" />

        <activity
                android:name=".UI.Activity.TermsAddActivity"
                android:exported="false"
                android:label="Part List"
                android:parentActivityName=".UI.Activity.TermsListActivity" />

        <activity
                android:name=".UI.Activity.TermsListActivity"
                android:exported="false"
                android:label="Home"
                android:parentActivityName=".UI.Activity.MainActivity" />
        <activity
                android:name=".UI.Activity.MainActivity"
                android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>